{
  "name": "smartLinkey",
  "version": "1.1.0",
  "description": "smartLinkey",
  "main": "main_process.dist.js",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "set port=7777 && cross-env DISABLE_ESLINT=true roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "build": "cross-env ESLINT=none roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*",
    "watch": "cross-env COMPRESS=none roadhog build --watch",
    "run-electron": "electron main_process.js",
    "dev": "npm-run-all --parallel watch run-electron",
    "prebuild": "python ./py/check_guid.py && prepare-electron-dep.bat",
    "pack": "electron-builder.cmd main_process.dist.js --dir",
    "rebuild-ffi": "cd ./node_modules/ffi/ && node-gyp rebuild --target=2.0.0 --arch=ia32 --target_arch=ia32 --dist-url=https://atom.io/download/electron && cd ../ref && node-gyp rebuild --target=2.0.0 --arch=ia32 --target_arch=ia32 --dist-url=https://atom.io/download/electron",
    "postinstall": "install-app-deps"
  },
  "author": "哈尔滨海邻科信息技术有限公司",
  "build": {
    "productName": "SmartLinkey",
    "compression": "normal",
    "electronVersion": "2.0.0",
    "directories": {
      "output": "./elect_builder_output"
    },
    "asar": true,
    "asarUnpack": [
      "dist"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/for-electron/source/logo.ico",
      "legalTrademarks": "HyLink",
      "files": [
        "!downloads",
        "!logs",
        "!resources",
        "!tests",
        "!mock",
        "!py",
        "!src",
        "!db.json",
        "!main_process.js",
        "!prepare-electron-dep.bat",
        "!*.md",
        "!*.log",
        "!.webpackrc.js",
        "!.stylelintrc",
        "!.roadhogrc.mock.js",
        "!.prettierrc",
        "!.prettierignore",
        "!.ga",
        "!.eslintrc.js",
        "!.eslintignore",
        "!.babelrc.js",
        "!LICENSE"
      ]
    },
    "nsis": {
      "shortcutName": "SmartLinkey",
      "guid": "50c1930e-15ef-5300-9367-f5b10795c0e4",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "extraFiles": [
      "IconExtractor.exe",
      "Newtonsoft.Json.dll",
      ".electron-crates.txt",
      "uplaunch.bat",
      "uplaunch.vbs",
      "fpatch.exe",
      "guid.txt",
      "bin",
      "angel.dll",
      "BottomHandle.exe",
      "mfc120.dll",
      "msvcp100.dll",
      "msvcp100d.dll",
      "msvcp120.dll",
      "msvcp120d.dll",
      "msvcr100.dll",
      "msvcr100d.dll",
      "msvcr110.dll",
      "msvcr120.dll",
      "msvcr120d.dll"
    ]
  },
  "dependencies": {
    "@antv/data-set": "^0.8.0",
    "@babel/polyfill": "^7.0.0-beta.36",
    "aes-js": "^3.1.2",
    "ant-design-pro": "^2.0.0",
    "antd": "^3.9.0",
    "arch": "^2.1.1",
    "auto-launch": "^5.0.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.9.2",
    "bizcharts": "^3.1.5",
    "bizcharts-plugin-slider": "^2.0.1",
    "bufferutil": "^3.0.5",
    "bunyan": "^1.8.12",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "dva": "^2.2.3",
    "dva-loading": "^2.0.3",
    "electron-localshortcut": "^3.1.0",
    "electron-pug": "^2.0.0",
    "enquire-js": "^0.2.1",
    "execa": "^0.10.0",
    "express": "^4.16.4",
    "ffi": "^2.2.0",
    "is-wsl": "^1.1.0",
    "less": "2.7.2",
    "lodash": "^4.17.10",
    "lodash-decorators": "^5.0.0",
    "lowdb": "^1.0.0",
    "md5-es": "^1.8.2",
    "md5-file": "^4.0.0",
    "moment": "^2.19.1",
    "node-rsa": "^1.0.5",
    "node-xmpp": "^1.1.0",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "opn": "^5.3.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-drawer": "^1.2.0",
    "react": "^16.2.0",
    "react-container-query": "^0.11.0",
    "react-cookie": "^2.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-fittext": "^1.0.0",
    "react-native-xmpp": "^1.0.3",
    "request": "^2.87.0",
    "request-progress": "^3.0.0",
    "rollbar": "^2.3.4",
    "socket.io-client": "^2.2.0",
    "strophe.js": "^1.2.15",
    "strophejs-plugin-pubsub": "0.0.2",
    "url-polyfill": "^1.0.10",
    "win-mouse": "^1.2.0",
    "windows-registry": "^0.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "electron": "2.0.0",
    "electron-reload": "^1.2.5",
    "enzyme": "^3.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mockjs": "^1.0.1-beta3",
    "npm-run-all": "^4.1.3",
    "prettier": "1.13.5",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.11.1",
    "roadhog": "^2.4.1",
    "roadhog-api-doc": "^1.0.3",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-config-standard": "^18.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
